<?xml version="1.0" encoding="utf-8"?>
<sg.bigo.ads.api.NativeAdView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/inter_native_ad_view"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:id="@+id/inter_media_container"
        android:background="#66000000"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <TextView
            android:textSize="13sp"
            android:textColor="#c7999999"
            android:ellipsize="end"
            android:gravity="center"
            android:id="@+id/inter_warning"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="160dp"
            android:layout_marginBottom="20dp"
            android:text=""
            android:maxLines="2"
            android:shadowColor="@color/black"
            android:shadowDx="1"
            android:shadowDy="1"
            android:shadowRadius="1"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"/>
        <RelativeLayout
            android:gravity="top|center_horizontal"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_above="@+id/inter_warning">
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="20dp">
                <LinearLayout
                    android:gravity="center"
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="20dp"
                    android:layout_weight="1"
                    android:layout_marginStart="20dp">
                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1">
                        <RelativeLayout
                            android:layout_gravity="center"
                            android:orientation="vertical"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">
                            <sg.bigo.ads.common.view.RoundedFrameLayout
                                android:id="@+id/media_container"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                app:radius="8dp">
                                <sg.bigo.ads.api.MediaView
                                    android:id="@+id/inter_media"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"/>
                                <Button
                                    android:layout_gravity="bottom|right"
                                    android:id="@+id/inter_btn_mute"
                                    android:background="@drawable/bigo_ad_ic_media_mute"
                                    android:visibility="invisible"
                                    android:layout_width="28dp"
                                    android:layout_height="28dp"
                                    android:layout_marginRight="12dp"
                                    android:layout_marginBottom="12dp"
                                    android:layout_marginEnd="12dp"/>
                            </sg.bigo.ads.common.view.RoundedFrameLayout>
                        </RelativeLayout>
                    </FrameLayout>
                    <sg.bigo.ads.common.view.RoundedFrameLayout
                        android:id="@+id/inter_btn_cta_main_layout"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:layout_marginTop="20dp"
                        android:layout_marginBottom="24dp"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentBottom="true"
                        android:layout_alignParentEnd="true"
                        app:radius="6dp">
                        <Button
                            android:textSize="14sp"
                            android:textColor="@android:color/white"
                            android:ellipsize="end"
                            android:id="@+id/inter_btn_cta_main"
                            android:background="#33ffffff"
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:text="@string/bigo_ad_cta_default"
                            android:maxLines="1"
                            android:textAllCaps="false"/>
                    </sg.bigo.ads.common.view.RoundedFrameLayout>
                </LinearLayout>
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="8dp"
                    android:layout_marginRight="20dp"
                    android:layout_weight="1"
                    android:layout_marginStart="8dp"
                    android:layout_marginEnd="20dp">
                    <FrameLayout
                        android:orientation="vertical"
                        android:id="@+id/inter_ad_info"
                        android:visibility="invisible"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1">
                        <sg.bigo.ads.common.view.RoundedFrameLayout
                            android:id="@+id/inter_ad_info_background"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">
                            <LinearLayout
                                android:layout_gravity="center"
                                android:orientation="vertical"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">
                                <LinearLayout
                                    android:gravity="center_vertical"
                                    android:orientation="horizontal"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="18dp"
                                    android:layout_marginEnd="32dp">
                                    <sg.bigo.ads.common.view.RoundedFrameLayout
                                        android:id="@+id/icon_layout"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginLeft="20dp"
                                        android:layout_marginStart="20dp"
                                        app:radius="8dp">
                                        <ImageView
                                            android:id="@+id/inter_icon"
                                            android:layout_width="72dp"
                                            android:layout_height="72dp"/>
                                    </sg.bigo.ads.common.view.RoundedFrameLayout>
                                    <LinearLayout
                                        android:orientation="vertical"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginLeft="20dp"
                                        android:layout_marginStart="20dp">
                                        <TextView
                                            android:textSize="21sp"
                                            android:textColor="#ffffffff"
                                            android:ellipsize="end"
                                            android:id="@+id/inter_title"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:text="@string/bigo_ad_title_default"
                                            android:lines="1"
                                            android:fontFamily="sans-serif-medium"/>
                                        <TextView
                                            android:textSize="13sp"
                                            android:textColor="#ffffffff"
                                            android:ellipsize="end"
                                            android:gravity="left"
                                            android:id="@+id/inter_company"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginTop="8dp"
                                            android:fontFamily="sans-serif-medium"
                                            android:layout_marginEnd="20dp"/>
                                        <TextView
                                            android:textSize="12sp"
                                            android:textColor="#9affffff"
                                            android:ellipsize="end"
                                            android:id="@+id/inter_description"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginTop="4dp"
                                            android:text="@string/bigo_ad_description_default"
                                            android:maxLines="2"/>
                                    </LinearLayout>
                                </LinearLayout>
                                <sg.bigo.ads.ad.interstitial.multi_img.view.IconListView
                                    android:id="@+id/download_msg_list"
                                    android:visibility="gone"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="20dp"/>
                            </LinearLayout>
                        </sg.bigo.ads.common.view.RoundedFrameLayout>
                    </FrameLayout>
                    <sg.bigo.ads.common.view.RoundedFrameLayout
                        android:id="@+id/inter_btn_cta_layout"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:layout_marginTop="20dp"
                        android:layout_marginRight="20dp"
                        android:layout_marginBottom="24dp"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentBottom="true"
                        android:layout_marginEnd="20dp"
                        android:layout_alignParentEnd="true"
                        app:radius="6dp">
                        <Button
                            android:textSize="14sp"
                            android:textColor="@android:color/white"
                            android:ellipsize="end"
                            android:id="@+id/inter_btn_cta"
                            android:background="@drawable/bigo_ad_button_bg_rectangle_blue"
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:text="@string/bigo_ad_cta_default"
                            android:maxLines="1"
                            android:textAllCaps="false"/>
                    </sg.bigo.ads.common.view.RoundedFrameLayout>
                </LinearLayout>
            </LinearLayout>
        </RelativeLayout>
        <sg.bigo.ads.api.AdOptionsView
            android:id="@+id/inter_options"
            android:layout_width="13dp"
            android:layout_height="13dp"
            android:layout_marginRight="9.5dp"
            android:layout_marginBottom="9.5dp"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="9.5dp"
            android:layout_alignParentEnd="true"/>
        <TextView
            android:textSize="9sp"
            android:textColor="#ccffffff"
            android:gravity="center"
            android:id="@+id/inter_ad_label"
            android:background="@drawable/bigo_adtag_background"
            android:paddingLeft="3dp"
            android:paddingRight="3dp"
            android:paddingBottom="1dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="7dp"
            android:layout_marginBottom="7dp"
            android:text="@string/ad"
            android:layout_alignParentLeft="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="7dp"/>
        <TextView
            android:textSize="13sp"
            android:textColor="#ffffffff"
            android:ellipsize="end"
            android:id="@+id/inter_advertiser"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginTop="8.5dp"
            android:text="bigoad.com"
            android:maxLines="1"/>
    </RelativeLayout>
</sg.bigo.ads.api.NativeAdView>
