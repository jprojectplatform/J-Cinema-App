<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/layout_end_page"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <FrameLayout
        android:id="@+id/inter_end_page"
        android:background="@drawable/bigo_ad_interstitial_rich_video_end_landscape_mask"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:gravity="center"
            android:layout_gravity="center"
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <sg.bigo.ads.common.view.RoundedFrameLayout xmlns:app="http://schemas.android.com/apk/res-auto"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:radius="22dp">
                <ImageView
                    android:id="@+id/inter_icon"
                    android:layout_width="100dp"
                    android:layout_height="100dp"/>
            </sg.bigo.ads.common.view.RoundedFrameLayout>
            <TextView
                android:textSize="21.5sp"
                android:textColor="#ff000000"
                android:ellipsize="end"
                android:gravity="center"
                android:id="@+id/inter_title"
                android:layout_width="311dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:text="@string/bigo_ad_title_default"
                android:maxLines="2"
                android:fontFamily="sans-serif-medium"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"/>
            <TextView
                android:textSize="13sp"
                android:textColor="#ff000000"
                android:ellipsize="end"
                android:gravity="center"
                android:id="@+id/inter_description"
                android:layout_width="311dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:text="@string/bigo_ad_description_default"
                android:maxLines="2"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"/>
            <sg.bigo.ads.common.view.RoundedFrameLayout
                android:id="@+id/inter_btn_end_page_cta_layout"
                android:layout_width="160dp"
                android:layout_height="40dp"
                android:layout_marginTop="24dp"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                app:radius="8dp">
                <Button
                    android:textSize="14sp"
                    android:textColor="@android:color/white"
                    android:ellipsize="end"
                    android:id="@+id/inter_btn_cta"
                    android:background="@drawable/bigo_ad_button_bg_rectangle_blue"
                    android:layout_width="160dp"
                    android:layout_height="40dp"
                    android:text="@string/bigo_ad_cta_default"
                    android:maxLines="1"
                    android:textAllCaps="false"/>
            </sg.bigo.ads.common.view.RoundedFrameLayout>
            <LinearLayout
                android:gravity="center"
                android:orientation="horizontal"
                android:id="@+id/layout_playable_loading"
                android:paddingTop="20dp"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <ProgressBar
                    android:layout_width="16dp"
                    android:layout_height="16dp"
                    android:indeterminateDrawable="@drawable/bigo_ad_progressbar_white"/>
                <TextView
                    android:textSize="14sp"
                    android:textColor="@android:color/white"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Playable ad will be shown soon"
                    android:maxLines="1"
                    android:layout_marginStart="6dp"/>
            </LinearLayout>
        </LinearLayout>
        <TextView
            android:textSize="12sp"
            android:textColor="#ffaeb3b7"
            android:ellipsize="end"
            android:gravity="center"
            android:id="@+id/inter_advertiser"
            android:background="@drawable/bigo_advertiser_background"
            android:paddingLeft="2dp"
            android:paddingTop="4dp"
            android:paddingRight="2dp"
            android:paddingBottom="4dp"
            android:visibility="gone"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginTop="-7dp"
            android:layout_marginBottom="20dp"
            android:maxWidth="340dp"
            android:text="bigoad.com"
            android:maxLines="1"/>
        <TextView
            android:textSize="13sp"
            android:textColor="#ffd8d8d8"
            android:ellipsize="end"
            android:gravity="center"
            android:id="@+id/inter_warning"
            android:visibility="gone"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginBottom="16dp"
            android:text=""
            android:maxLines="2"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"/>
        <TextView
            android:textSize="9sp"
            android:textColor="#ccffffff"
            android:gravity="center"
            android:layout_gravity="start|bottom"
            android:id="@+id/inter_ad_label"
            android:background="@drawable/bigo_adtag_background"
            android:paddingLeft="3dp"
            android:paddingTop="1dp"
            android:paddingRight="3dp"
            android:paddingBottom="1dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="8dp"
            android:text="@string/ad"/>
        <sg.bigo.ads.api.AdOptionsView
            android:layout_gravity="end|bottom"
            android:id="@+id/inter_options"
            android:layout_width="16dp"
            android:layout_height="16dp"
            android:layout_margin="9.5dp"/>
    </FrameLayout>
</FrameLayout>
