<?xml version="1.0" encoding="utf-8"?>
<sg.bigo.ads.api.NativeAdView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/inter_native_ad_view"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:orientation="horizontal"
        android:id="@+id/inter_ad_info"
        android:visibility="invisible"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <RelativeLayout
        android:background="#ff000000"
        android:paddingBottom="8dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <sg.bigo.ads.common.view.RoundedFrameLayout
            android:layout_gravity="center"
            android:id="@+id/inter_media_container"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="10dp"
            android:layout_above="@+id/other_layout"
            app:radius="12dp">
            <RelativeLayout
                android:gravity="top|center_horizontal"
                android:orientation="vertical"
                android:id="@+id/inter_ad_info_new"
                android:background="#ffffffff"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <sg.bigo.ads.common.view.RoundedFrameLayout
                        android:id="@+id/inter_media_layout"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="20dp"
                        android:layout_marginRight="20dp"
                        android:layout_marginBottom="28dp"
                        android:layout_above="@+id/inter_title"
                        android:layout_below="@+id/icon_layout"
                        app:radius="4dp">
                        <sg.bigo.ads.api.MediaView
                            android:id="@+id/inter_media"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_centerInParent="true"/>
                    </sg.bigo.ads.common.view.RoundedFrameLayout>
                    <LinearLayout
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:id="@+id/icon_layout"
                        android:paddingLeft="20dp"
                        android:paddingTop="24dp"
                        android:paddingRight="60dp"
                        android:paddingBottom="20dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingStart="20dp"
                        android:paddingEnd="60dp">
                        <sg.bigo.ads.common.view.RoundedFrameLayout
                            android:layout_gravity="center"
                            android:background="@drawable/bigo_ad_icon_default_radius6"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:radius="6dp">
                            <ImageView
                                android:id="@+id/inter_icon"
                                android:layout_width="32dp"
                                android:layout_height="32dp"/>
                        </sg.bigo.ads.common.view.RoundedFrameLayout>
                        <TextView
                            android:textSize="15sp"
                            android:textColor="#ff333333"
                            android:ellipsize="end"
                            android:gravity="left"
                            android:id="@+id/inter_company"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="16dp"
                            android:fontFamily="sans-serif-medium"
                            android:layout_marginStart="16dp"/>
                    </LinearLayout>
                    <TextView
                        android:textSize="21.5sp"
                        android:textColor="#ff202124"
                        android:ellipsize="end"
                        android:id="@+id/inter_title"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/bigo_ad_title_default"
                        android:maxLines="2"
                        android:layout_above="@+id/inter_description"
                        android:fontFamily="sans-serif-medium"
                        android:layout_marginStart="20dp"
                        android:layout_marginEnd="20dp"/>
                    <TextView
                        android:textSize="12sp"
                        android:textColor="#ff5f6367"
                        android:ellipsize="end"
                        android:gravity="left"
                        android:id="@+id/inter_description"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="8dp"
                        android:text="@string/bigo_ad_description_default"
                        android:maxLines="2"
                        android:layout_above="@+id/btn_class"
                        android:fontFamily="sans-serif-medium"
                        android:layout_marginStart="20dp"
                        android:layout_marginEnd="20dp"/>
                    <LinearLayout
                        android:gravity="center"
                        android:orientation="vertical"
                        android:id="@+id/btn_class"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true">
                        <sg.bigo.ads.common.view.RoundedFrameLayout
                            android:layout_gravity="center"
                            android:id="@+id/inter_btn_cta_layout"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="20dp"
                            android:layout_marginTop="24dp"
                            android:layout_marginRight="20dp"
                            android:layout_marginBottom="16dp"
                            android:layout_marginStart="20dp"
                            android:layout_marginEnd="20dp"
                            app:radius="8dp">
                            <Button
                                android:textSize="14sp"
                                android:textColor="@android:color/white"
                                android:ellipsize="end"
                                android:id="@+id/inter_btn_cta"
                                android:background="@drawable/bigo_ad_button_bg_rectangle_blue"
                                android:layout_width="match_parent"
                                android:layout_height="40dp"
                                android:text="@string/bigo_ad_cta_default"
                                android:layout_weight="1"
                                android:textAllCaps="false"/>
                        </sg.bigo.ads.common.view.RoundedFrameLayout>
                    </LinearLayout>
                </RelativeLayout>
            </RelativeLayout>
        </sg.bigo.ads.common.view.RoundedFrameLayout>
        <RelativeLayout
            android:id="@+id/other_layout"
            android:paddingBottom="8dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_alignParentBottom="true">
            <TextView
                android:textSize="13sp"
                android:textColor="#ff999999"
                android:ellipsize="end"
                android:gravity="center"
                android:id="@+id/inter_warning"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="3dp"
                android:maxLines="2"
                android:textAlignment="center"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"/>
            <sg.bigo.ads.api.AdOptionsView
                android:id="@+id/inter_options"
                android:layout_width="16dp"
                android:layout_height="16dp"
                android:layout_marginRight="8dp"
                android:layout_below="@+id/inter_warning"
                android:layout_alignParentEnd="true"/>
            <include
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="8dp"
                android:layout_alignBottom="@+id/inter_options"
                android:layout_alignParentStart="true"
                layout="@layout/bigo_ad_label_layout"/>
        </RelativeLayout>
    </RelativeLayout>
</sg.bigo.ads.api.NativeAdView>
