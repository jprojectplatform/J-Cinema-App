<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/rl_top"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ImageView
        android:id="@+id/iv_feedback_close"
        android:layout_width="@dimen/dp_21"
        android:layout_height="@dimen/dp_21"
        android:layout_marginTop="@dimen/dp_150"
        android:layout_marginRight="@dimen/dp_50"
        android:src="@drawable/ic_close1"
        android:layout_alignParentRight="true"/>
    <RelativeLayout
        android:orientation="vertical"
        android:background="@drawable/bg_show_feedback_once"
        android:layout_width="@dimen/dp_257"
        android:layout_height="@dimen/dp_306"
        android:layout_marginTop="@dimen/dp_15"
        android:layout_below="@+id/iv_feedback_close"
        android:layout_centerHorizontal="true">
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="@dimen/dp_161"
            android:layout_marginTop="@dimen/dp_30"
            android:src="@drawable/ic_copy_code_video_bg"
            android:layout_centerHorizontal="true"/>
        <com.mgs.carparking.background.RoundedImageView
            android:id="@+id/iv_image"
            android:layout_width="@dimen/dp_117"
            android:layout_height="@dimen/dp_161"
            android:layout_marginTop="@dimen/dp_30"
            android:src="@drawable/ic_video_default"
            android:scaleType="centerCrop"
            android:layout_centerHorizontal="true"/>
        <TextView
            android:textSize="@dimen/sp_16"
            android:textColor="@color/color_text_commen1"
            android:gravity="center"
            android:id="@+id/tv_name"
            android:paddingLeft="@dimen/dp_20"
            android:paddingRight="@dimen/dp_20"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_10"
            android:layout_below="@+id/iv_image"/>
        <TextView
            android:textSize="@dimen/sp_12"
            android:textColor="@color/color_666666"
            android:ellipsize="end"
            android:gravity="center"
            android:id="@+id/tv_actor"
            android:paddingLeft="@dimen/dp_20"
            android:paddingRight="@dimen/dp_20"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_5"
            android:maxLines="1"
            android:layout_below="@+id/tv_name"/>
        <TextView
            android:textSize="@dimen/sp_16"
            android:textStyle="bold"
            android:textColor="@color/white"
            android:gravity="center"
            android:layout_gravity="center"
            android:id="@+id/tv_feedback_submit"
            android:background="@drawable/bg_show_one_video_open"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_42"
            android:layout_marginTop="@dimen/dp_22"
            android:text="@string/str_continue_look"
            android:layout_alignParentBottom="true"/>
    </RelativeLayout>
</RelativeLayout>
