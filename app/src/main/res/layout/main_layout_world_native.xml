<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:gravity="center_vertical"
    android:orientation="horizontal"
    android:id="@+id/main_cp_native_content"
    android:background="#ff191c1e"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dp_135">
    <RelativeLayout
        android:id="@+id/main_cp_native_icon_rl"
        android:layout_width="@dimen/dp_90"
        android:layout_height="@dimen/dp_135">
        <com.yk.e.adview.RoundRectImageView
            android:id="@+id/main_cp_native_rr_icon"
            android:visibility="visible"
            android:layout_width="@dimen/dp_90"
            android:layout_height="@dimen/dp_135"
            android:layout_centerInParent="true"/>
        <pl.droidsonroids.gif.GifImageView
            android:id="@+id/main_cp_native_icon"
            android:visibility="gone"
            android:layout_width="@dimen/dp_90"
            android:layout_height="@dimen/dp_135"
            android:layout_centerInParent="true"/>
        <TextView
            android:textSize="10sp"
            android:paddingBottom="5dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/main_ad"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="10dp"/>
    </RelativeLayout>
    <LinearLayout
        android:gravity="center_vertical"
        android:orientation="vertical"
        android:id="@+id/main_cp_native_center_ly"
        android:paddingTop="15dp"
        android:paddingBottom="@dimen/dp_7"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="@dimen/dp_14">
        <TextView
            android:textSize="@dimen/sp_15"
            android:textColor="#ccffffff"
            android:ellipsize="end"
            android:id="@+id/main_cp_native_title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/dp_12"
            android:text=""
            android:maxLines="1"/>
        <TextView
            android:textSize="@dimen/sp_12"
            android:textColor="#ff999999"
            android:ellipsize="end"
            android:id="@+id/main_cp_native_title2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_8"
            android:layout_marginRight="@dimen/dp_12"
            android:text=""
            android:maxLines="1"/>
        <TextView
            android:textSize="@dimen/sp_12"
            android:textColor="#ff666666"
            android:ellipsize="end"
            android:id="@+id/main_cp_native_details"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_8"
            android:layout_marginRight="@dimen/dp_12"
            android:text=""
            android:maxLines="2"/>
        <LinearLayout
            android:gravity="bottom|right"
            android:orientation="horizontal"
            android:id="@+id/main_cp_native_tag_ly"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="@dimen/dp_6"
            android:layout_marginRight="@dimen/dp_9">
            <TextView
                android:textSize="@dimen/sp_10"
                android:textColor="#b0999999"
                android:ellipsize="end"
                android:gravity="center"
                android:layout_gravity="center"
                android:id="@+id/main_cp_native_tag1"
                android:background="@drawable/main_bg_native_tag_tv"
                android:paddingLeft="@dimen/dp_4"
                android:paddingTop="@dimen/dp_2"
                android:paddingRight="@dimen/dp_4"
                android:paddingBottom="@dimen/dp_2"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="5dp"
                android:maxLines="1"/>
            <TextView
                android:textSize="@dimen/sp_10"
                android:textColor="#b0999999"
                android:ellipsize="end"
                android:gravity="center"
                android:layout_gravity="center"
                android:id="@+id/main_cp_native_tag2"
                android:background="@drawable/main_bg_native_tag_tv"
                android:paddingLeft="@dimen/dp_4"
                android:paddingTop="@dimen/dp_2"
                android:paddingRight="@dimen/dp_4"
                android:paddingBottom="@dimen/dp_2"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="5dp"
                android:maxLines="1"/>
            <TextView
                android:textSize="@dimen/sp_10"
                android:textColor="#b0999999"
                android:ellipsize="end"
                android:gravity="center"
                android:layout_gravity="center"
                android:id="@+id/main_cp_native_tag3"
                android:background="@drawable/main_bg_native_tag_tv"
                android:paddingLeft="@dimen/dp_4"
                android:paddingTop="@dimen/dp_2"
                android:paddingRight="@dimen/dp_4"
                android:paddingBottom="@dimen/dp_2"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:maxLines="1"/>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>
