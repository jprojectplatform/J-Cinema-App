<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:tag="layout/item_mine_history_0"
    android:paddingLeft="@dimen/dp_9"
    android:paddingRight="@dimen/dp_9"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dp_102"
    android:layout_marginBottom="@dimen/dp_15">
    <RelativeLayout
        android:id="@+id/layout_check"
        android:tag="binding_1"
        android:layout_width="@dimen/dp_40"
        android:layout_height="match_parent">
        <CheckBox
            android:gravity="center"
            android:tag="binding_2"
            android:clickable="false"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:button="@drawable/ic_check_history"
            android:layout_centerInParent="true"/>
    </RelativeLayout>
    <com.mgs.carparking.background.RoundedImageView
        android:id="@+id/item_img"
        android:tag="binding_3"
        android:layout_width="@dimen/dp_68"
        android:layout_height="@dimen/dp_102"
        android:scaleType="centerCrop"
        android:layout_toRightOf="@+id/layout_check"/>
    <LinearLayout
        android:gravity="center_vertical"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="@dimen/dp_14"
        android:layout_toRightOf="@+id/item_img">
        <TextView
            android:textSize="@dimen/sp_14"
            android:textColor="@color/color_text_commen1"
            android:ellipsize="end"
            android:tag="binding_4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:maxLines="2"
            android:layout_centerVertical="true"/>
        <TextView
            android:textSize="@dimen/sp_11"
            android:textColor="@color/color_commen"
            android:ellipsize="end"
            android:tag="binding_5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_21"
            android:maxLines="2"/>
    </LinearLayout>
</RelativeLayout>
