<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:fresco="http://schemas.android.com/apk/res-auto"
    android:id="@+id/background"
    android:background="#33333333"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_gravity="bottom"
        android:orientation="vertical"
        android:id="@+id/layout_start"
        android:background="@drawable/bigo_ad_feedback_background"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <View
            android:layout_gravity="center_horizontal"
            android:background="@drawable/bigo_ad_feedback_top_slide_bar"
            android:layout_width="40dp"
            android:layout_height="4dp"
            android:layout_marginTop="6dp"
            android:layout_marginBottom="6dp"/>
        <FrameLayout
            android:gravity="start|center_vertical"
            android:orientation="horizontal"
            android:id="@+id/btn_report"
            android:focusable="true"
            android:visibility="gone"
            android:clickable="true"
            android:layout_width="match_parent"
            android:layout_height="56dp">
            <TextView
                android:textSize="17sp"
                android:textColor="#ff333333"
                android:ellipsize="end"
                android:gravity="center"
                android:layout_gravity="start|center_vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="20dp"
                android:text="@string/bigo_ad_feedback_report_ads"
                android:lines="1"
                android:fontFamily="sans-serif-medium"
                android:layout_marginStart="15dp"
                android:layout_marginEnd="20dp"/>
            <View
                android:gravity="center"
                android:layout_gravity="end|center_vertical"
                android:background="@drawable/bigo_ad_feeback_arrow_right"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="20dp"
                android:layout_marginStart="15dp"
                android:layout_marginEnd="20dp"/>
        </FrameLayout>
        <FrameLayout
            android:id="@+id/btn_hide"
            android:focusable="true"
            android:visibility="gone"
            android:clickable="true"
            android:layout_width="match_parent"
            android:layout_height="56dp">
            <View
                android:id="@+id/line"
                android:background="#ffe9e9e9"
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="15dp"
                android:layout_marginStart="15dp"/>
            <TextView
                android:textSize="17sp"
                android:textColor="#ff333333"
                android:ellipsize="end"
                android:gravity="center"
                android:layout_gravity="start|center_vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="20dp"
                android:text="@string/bigo_ad_feedback_hide_ads"
                android:lines="1"
                android:fontFamily="sans-serif-medium"
                android:layout_marginStart="15dp"
                android:layout_marginEnd="20dp"/>
        </FrameLayout>
        <FrameLayout
            android:id="@+id/btn_why_this_ad"
            android:focusable="true"
            android:visibility="gone"
            android:clickable="true"
            android:layout_width="match_parent"
            android:layout_height="56dp">
            <View
                android:id="@+id/line"
                android:background="#ffe9e9e9"
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="15dp"
                android:layout_marginStart="15dp"/>
            <TextView
                android:textSize="17sp"
                android:textColor="#ff333333"
                android:ellipsize="end"
                android:gravity="center"
                android:layout_gravity="start|center_vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="20dp"
                android:text="@string/bigo_ad_feedback_why_this_ad"
                android:lines="1"
                android:fontFamily="sans-serif-medium"
                android:layout_marginStart="15dp"
                android:layout_marginEnd="20dp"/>
            <View
                android:gravity="center"
                android:layout_gravity="end|center_vertical"
                android:background="@drawable/bigo_ad_feeback_arrow_right"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="20dp"
                android:layout_marginStart="15dp"
                android:layout_marginEnd="20dp"/>
        </FrameLayout>
        <FrameLayout
            android:id="@+id/btn_copy_ru_ad_marker"
            android:focusable="true"
            android:visibility="gone"
            android:clickable="true"
            android:layout_width="match_parent"
            android:layout_height="56dp">
            <View
                android:id="@+id/line"
                android:background="#ffe9e9e9"
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="15dp"
                android:layout_marginStart="15dp"/>
            <TextView
                android:textSize="17sp"
                android:textColor="#ff333333"
                android:ellipsize="end"
                android:gravity="center"
                android:layout_gravity="start|center_vertical"
                android:id="@+id/bigo_ad_text_copy_ru_ad_marker"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="20dp"
                android:text="@string/bigo_ad_feedback_copy_ad_id"
                android:lines="1"
                android:fontFamily="sans-serif-medium"
                android:layout_marginStart="15dp"
                android:layout_marginEnd="20dp"/>
        </FrameLayout>
    </LinearLayout>
</FrameLayout>
