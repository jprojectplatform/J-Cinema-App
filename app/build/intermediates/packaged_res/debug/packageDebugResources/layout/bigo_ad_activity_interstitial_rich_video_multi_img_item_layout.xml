<?xml version="1.0" encoding="utf-8"?>
<sg.bigo.ads.common.view.RoundedFrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/inter_media_item_container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:radius="8dp">
    <sg.bigo.ads.common.view.AdImageView
        android:id="@+id/inter_media_item_background"
        android:visibility="visible"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scaleType="centerCrop"/>
    <sg.bigo.ads.common.view.FixContentFrameLayout
        android:layout_gravity="center"
        android:id="@+id/inter_media_item_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:radius="8dp">
        <sg.bigo.ads.common.view.AdImageView
            android:layout_gravity="center"
            android:id="@+id/inter_media_item"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scaleType="fitCenter"/>
    </sg.bigo.ads.common.view.FixContentFrameLayout>
</sg.bigo.ads.common.view.RoundedFrameLayout>
