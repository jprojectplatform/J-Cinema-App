<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="@android:color/white"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ImageView
        android:layout_gravity="right"
        android:id="@+id/btn_close"
        android:background="@drawable/bigo_ad_ic_close_gray_light"
        android:padding="4dp"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_marginTop="20dp"
        android:layout_marginRight="25dp"
        android:layout_marginBottom="4dp"/>
    <FrameLayout
        android:id="@+id/native_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <LinearLayout
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <LinearLayout
                    android:gravity="center"
                    android:orientation="vertical"
                    android:paddingTop="40dp"
                    android:paddingBottom="64dp"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:layout_marginStart="40dp"
                    android:layout_marginEnd="20dp">
                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">
                        <sg.bigo.ads.common.view.RoundedFrameLayout
                            android:id="@+id/fl_icon"
                            android:layout_width="72dp"
                            android:layout_height="72dp"
                            android:layout_marginRight="20dp"
                            app:radius="16dp">
                            <sg.bigo.ads.common.view.AdImageView
                                android:id="@+id/iv_icon"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"/>
                        </sg.bigo.ads.common.view.RoundedFrameLayout>
                        <LinearLayout
                            android:layout_gravity="center_vertical"
                            android:orientation="vertical"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">
                            <TextView
                                android:textSize="21.5sp"
                                android:textColor="#ff202124"
                                android:ellipsize="end"
                                android:id="@+id/tv_title"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="4dp"
                                android:maxLines="2"
                                android:fontFamily="sans-serif-medium"/>
                            <TextView
                                android:textSize="13sp"
                                android:textColor="#ff1b8863"
                                android:ellipsize="end"
                                android:id="@+id/tv_company_name"
                                android:visibility="gone"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="4.5dp"
                                android:maxLines="1"/>
                            <TextView
                                android:textSize="12sp"
                                android:textColor="#ff5f6367"
                                android:ellipsize="end"
                                android:id="@+id/tv_desc"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="4dp"
                                android:maxLines="2"/>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:gravity="center"
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="30dp">
                        <LinearLayout
                            android:gravity="center"
                            android:orientation="vertical"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="20dp"
                            android:layout_marginRight="20dp">
                            <LinearLayout
                                android:gravity="center_vertical"
                                android:orientation="horizontal"
                                android:id="@+id/ll_start_rate"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content">
                                <TextView
                                    android:textSize="14sp"
                                    android:textColor="#ff202124"
                                    android:id="@+id/tv_start_rate"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"/>
                                <ImageView
                                    android:layout_gravity="center_vertical"
                                    android:background="@drawable/bigo_ad_ic_star"
                                    android:layout_width="14dp"
                                    android:layout_height="14dp"
                                    android:layout_marginLeft="4dp"/>
                            </LinearLayout>
                            <TextView
                                android:textSize="12sp"
                                android:textColor="#ff65696d"
                                android:ellipsize="marquee"
                                android:id="@+id/tv_comment"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="4dp"/>
                        </LinearLayout>
                        <View
                            android:background="#ffe7eaed"
                            android:layout_width="1dp"
                            android:layout_height="24dp"/>
                        <LinearLayout
                            android:gravity="center"
                            android:orientation="vertical"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="20dp"
                            android:layout_marginRight="20dp">
                            <LinearLayout
                                android:gravity="center"
                                android:orientation="horizontal"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content">
                                <TextView
                                    android:textSize="14sp"
                                    android:textColor="#ff202124"
                                    android:id="@+id/tv_download_num"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"/>
                            </LinearLayout>
                            <TextView
                                android:textSize="12sp"
                                android:textColor="#ff65696d"
                                android:ellipsize="marquee"
                                android:id="@+id/tv_download_num_desc"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="4dp"
                                android:text="@string/bigo_ad_download_num_text"/>
                        </LinearLayout>
                        <View
                            android:background="#ffe7eaed"
                            android:layout_width="1dp"
                            android:layout_height="24dp"/>
                        <LinearLayout
                            android:gravity="center"
                            android:orientation="vertical"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="20dp"
                            android:layout_marginRight="20dp">
                            <LinearLayout
                                android:gravity="center_vertical"
                                android:orientation="horizontal"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content">
                                <TextView
                                    android:textSize="14sp"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"/>
                                <ImageView
                                    android:id="@+id/iv_age"
                                    android:background="@drawable/bigo_ad_ic_everyone"
                                    android:layout_width="14dp"
                                    android:layout_height="14dp"
                                    android:layout_marginLeft="5dp"/>
                            </LinearLayout>
                            <LinearLayout
                                android:gravity="center_vertical"
                                android:orientation="horizontal"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="4dp">
                                <TextView
                                    android:textSize="12sp"
                                    android:textColor="#ff65696d"
                                    android:ellipsize="marquee"
                                    android:id="@+id/tv_age"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"/>
                                <ImageView
                                    android:layout_width="10dp"
                                    android:layout_height="10dp"
                                    android:layout_marginLeft="3dp"
                                    android:src="@drawable/bigo_ad_ic_everyone_ic_info"/>
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>
                    <sg.bigo.ads.common.view.RoundedFrameLayout
                        android:id="@+id/btn_cta"
                        android:layout_width="280dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="36dp"
                        app:radius="8dp">
                        <TextView
                            android:textSize="14sp"
                            android:textColor="@color/white"
                            android:gravity="center"
                            android:id="@+id/btn_cta_inner"
                            android:background="#ff1b8863"
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:text="@string/bigo_ad_cta_download_default"/>
                    </sg.bigo.ads.common.view.RoundedFrameLayout>
                </LinearLayout>
                <FrameLayout
                    android:paddingBottom="32dp"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1">
                    <sg.bigo.ads.common.view.RoundedFrameLayout
                        android:layout_gravity="center"
                        android:id="@+id/ll_fallback_media"
                        android:visibility="gone"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="20dp"
                        android:layout_marginEnd="40dp"
                        app:radius="4dp">
                        <sg.bigo.ads.common.view.AdImageView
                            android:id="@+id/iv_fallback_media"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>
                    </sg.bigo.ads.common.view.RoundedFrameLayout>
                </FrameLayout>
            </LinearLayout>
            <HorizontalScrollView
                android:orientation="horizontal"
                android:id="@+id/ll_media"
                android:visibility="gone"
                android:scrollbars="none"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_marginTop="40dp"/>
            <LinearLayout
                android:orientation="vertical"
                android:id="@+id/ll_native_extra"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="28dp"
                    android:layout_marginRight="20dp">
                    <TextView
                        android:textSize="16sp"
                        android:textColor="#ff202124"
                        android:layout_gravity="center"
                        android:id="@+id/tv_gp_info_extra_about"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/bigo_ad_about_app_default"/>
                    <ImageView
                        android:layout_gravity="center"
                        android:id="@+id/iv_gp_info_extra_arrow"
                        android:background="@drawable/bigo_ad_mid_page_ic_gp_right"
                        android:layout_width="16dp"
                        android:layout_height="16dp"/>
                </FrameLayout>
                <TextView
                    android:textSize="13sp"
                    android:textColor="#ff63666b"
                    android:ellipsize="end"
                    android:id="@+id/tv_desc_below"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginRight="20dp"/>
                <sg.bigo.ads.common.view.AutoNextLineLinearLayout
                    android:id="@+id/fbl_genre"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginRight="20dp"
                    android:layout_marginBottom="28dp"/>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</LinearLayout>
