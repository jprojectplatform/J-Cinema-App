<?xml version="1.0" encoding="utf-8"?>
<sg.bigo.ads.api.NativeAdView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/inter_native_ad_view"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:gravity="top|center_horizontal"
        android:orientation="vertical"
        android:id="@+id/inter_media_container"
        android:clipChildren="false"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <sg.bigo.ads.api.MediaView
            android:id="@+id/inter_media"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"/>
        <Button
            android:id="@+id/inter_btn_mute"
            android:background="@drawable/bigo_ad_ic_media_mute"
            android:visibility="invisible"
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:layout_marginBottom="12dp"
            android:layout_alignBottom="@+id/inter_media"
            android:layout_marginEnd="12dp"
            android:layout_alignEnd="@+id/inter_media"/>
        <TextView
            android:textSize="12sp"
            android:textColor="#ffe9ebec"
            android:ellipsize="end"
            android:id="@+id/inter_advertiser"
            android:background="@drawable/bigo_advertiser_background"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="18dp"
            android:layout_marginBottom="15dp"
            android:maxWidth="240dp"
            android:text="bigoad.com"
            android:maxLines="1"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="18dp"/>
        <FrameLayout
            android:orientation="vertical"
            android:id="@+id/inter_ad_info"
            android:visibility="invisible"
            android:clipChildren="false"
            android:clipToPadding="false"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true">
            <sg.bigo.ads.common.view.RoundedFrameLayout
                android:id="@+id/inter_ad_info_background"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <FrameLayout
                    android:background="#d9ffffff"
                    android:paddingBottom="8dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="16dp">
                        <TextView
                            android:textSize="20sp"
                            android:textColor="#ff333333"
                            android:ellipsize="end"
                            android:gravity="center"
                            android:id="@+id/inter_title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="56dp"
                            android:text="@string/bigo_ad_title_default"
                            android:lines="1"
                            android:layout_centerHorizontal="true"
                            android:fontFamily="sans-serif-medium"/>
                        <TextView
                            android:textSize="13sp"
                            android:textColor="#ff888888"
                            android:ellipsize="end"
                            android:gravity="center"
                            android:id="@+id/inter_description"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="8dp"
                            android:text="@string/bigo_ad_description_default"
                            android:lines="1"
                            android:layout_below="@+id/inter_title"
                            android:layout_centerHorizontal="true"
                            android:layout_marginStart="20dp"/>
                        <RelativeLayout
                            android:gravity="center"
                            android:id="@+id/btn_class"
                            android:padding="10dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5.5dp"
                            android:layout_below="@+id/inter_description"
                            android:layout_centerHorizontal="true">
                            <Button
                                android:textSize="14sp"
                                android:textColor="@android:color/white"
                                android:ellipsize="end"
                                android:id="@+id/inter_btn_cta_main"
                                android:background="@drawable/bigo_ad_button_bg_rectangle_green"
                                android:layout_width="168dp"
                                android:layout_height="44dp"
                                android:text="@string/bigo_ad_cta_default"
                                android:maxLines="1"
                                android:textAllCaps="false"/>
                            <sg.bigo.ads.common.view.RoundedFrameLayout xmlns:app="http://schemas.android.com/apk/res-auto"
                                android:id="@+id/inter_btn_cta_layout"
                                android:layout_width="168dp"
                                android:layout_height="44dp"
                                android:layout_marginLeft="10dp"
                                android:layout_marginStart="10dp"
                                android:layout_toEndOf="@+id/inter_btn_cta_main"
                                app:radius="6dp">
                                <Button
                                    android:textSize="14sp"
                                    android:textColor="@android:color/white"
                                    android:ellipsize="end"
                                    android:id="@+id/inter_btn_cta"
                                    android:background="@drawable/bigo_ad_button_bg_rectangle_blue"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:text="@string/bigo_ad_cta_default"
                                    android:maxLines="1"
                                    android:textAllCaps="false"/>
                            </sg.bigo.ads.common.view.RoundedFrameLayout>
                        </RelativeLayout>
                        <TextView
                            android:textSize="13sp"
                            android:textColor="#ffd8d8d8"
                            android:ellipsize="end"
                            android:gravity="center"
                            android:id="@+id/inter_warning"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="11dp"
                            android:text=""
                            android:maxLines="2"
                            android:layout_below="@+id/btn_class"
                            android:layout_marginStart="20dp"
                            android:layout_marginEnd="20dp"/>
                    </RelativeLayout>
                    <RelativeLayout
                        android:layout_gravity="end|bottom"
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="16dp">
                        <sg.bigo.ads.api.AdOptionsView
                            android:id="@+id/inter_options"
                            android:layout_width="16dp"
                            android:layout_height="16dp"
                            android:layout_marginRight="8dp"
                            android:layout_alignParentEnd="true"/>
                    </RelativeLayout>
                </FrameLayout>
            </sg.bigo.ads.common.view.RoundedFrameLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <sg.bigo.ads.common.view.RoundedFrameLayout
                    android:id="@+id/icon_layout"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="-40dp"
                    android:layout_centerHorizontal="true"
                    app:radius="6dp">
                    <ImageView
                        android:id="@+id/inter_icon"
                        android:layout_width="80dp"
                        android:layout_height="80dp"/>
                </sg.bigo.ads.common.view.RoundedFrameLayout>
            </RelativeLayout>
        </FrameLayout>
        <TextView
            android:textSize="9sp"
            android:textColor="#ccffffff"
            android:gravity="center"
            android:id="@+id/inter_ad_label"
            android:background="@drawable/bigo_adtag_background"
            android:paddingLeft="3dp"
            android:paddingTop="1dp"
            android:paddingRight="3dp"
            android:paddingBottom="1dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="8dp"
            android:text="@string/ad"/>
    </RelativeLayout>
</sg.bigo.ads.api.NativeAdView>
