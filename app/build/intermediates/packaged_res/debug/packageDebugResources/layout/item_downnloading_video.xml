<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="horizontal"
    android:tag="layout/item_downnloading_video_0"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dp_102"
    android:layout_marginBottom="@dimen/dp_10">
    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="@dimen/dp_102">
        <RelativeLayout
            android:id="@+id/layout_check"
            android:tag="binding_1"
            android:layout_width="@dimen/dp_40"
            android:layout_height="match_parent">
            <CheckBox
                android:gravity="center"
                android:tag="binding_2"
                android:clickable="false"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:button="@drawable/ic_check_history"
                android:layout_centerInParent="true"/>
        </RelativeLayout>
        <com.mgs.carparking.background.RoundedImageView
            android:tag="binding_3"
            android:layout_width="@dimen/dp_68"
            android:layout_height="@dimen/dp_102"
            android:scaleType="centerCrop"
            android:layout_toRightOf="@+id/layout_check"/>
    </RelativeLayout>
    <LinearLayout
        android:gravity="center_vertical"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="@dimen/dp_13"
        android:layout_marginRight="@dimen/dp_12">
        <TextView
            android:textSize="@dimen/dp_15"
            android:textStyle="bold"
            android:textColor="@color/color_text_commen1"
            android:ellipsize="end"
            android:tag="binding_4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:maxLines="1"/>
        <ProgressBar
            android:id="@+id/exo_video_audio_brightness_pro"
            android:tag="binding_5"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_3"
            android:layout_marginTop="@dimen/dp_20"
            android:max="100"
            style="@android:style/Widget.ProgressBar.Horizontal"/>
        <RelativeLayout
            android:gravity="center_vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:textSize="@dimen/sp_11"
                android:textColor="@color/color_666666"
                android:tag="binding_6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_5"
                android:layout_centerVertical="true"/>
            <TextView
                android:textSize="@dimen/sp_11"
                android:textColor="@color/color_666666"
                android:tag="binding_7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_4"
                android:layout_centerVertical="true"/>
            <TextView
                android:textSize="@dimen/sp_11"
                android:textColor="@color/color_666666"
                android:ellipsize="end"
                android:tag="binding_8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_10"
                android:maxLines="1"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"/>
        </RelativeLayout>
    </LinearLayout>
</LinearLayout>
