<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:tag="layout/activity_setting_0"
    android:background="@color/theme_bg_color"
    android:fitsSystemWindows="true"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:orientation="vertical"
        android:tag="binding_1"
        android:background="@color/theme_bg_color"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <include
            android:id="@+id/actionbar"
            layout="@layout/actionbar_back"/>
        <RelativeLayout
            android:tag="binding_2"
            android:background="@drawable/bg_pressed"
            android:paddingRight="@dimen/dp_10"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_40"
            android:layout_marginTop="@dimen/dp_20">
            <TextView
                android:textSize="@dimen/sp_15"
                android:textColor="@color/color_text_commen1"
                android:gravity="center_vertical"
                android:paddingLeft="@dimen/dp_15"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="@string/text_mine_update"
                android:drawableLeft="@drawable/ic_ming_update"
                android:drawablePadding="@dimen/dp_12"
                android:layout_centerVertical="true"/>
            <ImageView
                android:id="@+id/iv_right"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_mine_right"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"/>
            <TextView
                android:textSize="@dimen/sp_10"
                android:textColor="@color/color_666666"
                android:tag="binding_3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/dp_7"
                android:layout_toLeftOf="@+id/iv_right"
                android:layout_centerVertical="true"/>
        </RelativeLayout>
        <RelativeLayout
            android:tag="binding_4"
            android:background="@drawable/bg_pressed"
            android:paddingLeft="@dimen/dp_15"
            android:paddingRight="@dimen/dp_10"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_40"
            android:layout_marginTop="@dimen/dp_10">
            <TextView
                android:textSize="@dimen/sp_15"
                android:textColor="@color/color_text_commen1"
                android:gravity="center_vertical"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="@string/text_mine_clear_cache"
                android:drawableLeft="@drawable/ic_mine_clear_cache"
                android:drawablePadding="@dimen/dp_12"
                android:layout_centerVertical="true"/>
            <ImageView
                android:id="@+id/iv_right1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_mine_right"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"/>
            <TextView
                android:textSize="@dimen/sp_10"
                android:textColor="@color/color_666666"
                android:tag="binding_5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/dp_7"
                android:layout_toLeftOf="@+id/iv_right1"
                android:layout_centerVertical="true"/>
        </RelativeLayout>
        <RelativeLayout
            android:tag="binding_6"
            android:background="@drawable/bg_pressed"
            android:paddingLeft="@dimen/dp_15"
            android:paddingRight="@dimen/dp_10"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_40"
            android:layout_marginTop="@dimen/dp_10">
            <TextView
                android:textSize="@dimen/sp_15"
                android:textColor="@color/color_text_commen1"
                android:gravity="center_vertical"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="@string/text_mine_score"
                android:drawableLeft="@drawable/ic_mine_score"
                android:drawablePadding="@dimen/dp_12"
                android:layout_centerVertical="true"/>
            <ImageView
                android:id="@+id/iv_right5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_mine_right"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"/>
        </RelativeLayout>
        <RelativeLayout
            android:tag="binding_7"
            android:background="@drawable/bg_pressed"
            android:paddingLeft="@dimen/dp_15"
            android:paddingRight="@dimen/dp_10"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_40"
            android:layout_marginTop="@dimen/dp_10">
            <TextView
                android:textSize="@dimen/sp_15"
                android:textColor="@color/color_text_commen1"
                android:gravity="center_vertical"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="@string/text_mine_privacy"
                android:drawableLeft="@drawable/ic_mine_privacy"
                android:drawablePadding="@dimen/dp_12"
                android:layout_centerVertical="true"/>
            <ImageView
                android:id="@+id/iv_right2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_mine_right"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"/>
        </RelativeLayout>
        <RelativeLayout
            android:tag="binding_8"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
    </LinearLayout>
    <Button
        android:textSize="@dimen/sp_17"
        android:textStyle="bold"
        android:textColor="@color/white"
        android:id="@+id/bt_submit"
        android:tag="binding_9"
        android:background="@drawable/shape_logout_submit_click"
        android:layout_width="@dimen/dp_308"
        android:layout_height="@dimen/dp_42"
        android:layout_marginBottom="@dimen/dp_80"
        android:text="@string/text_login_logout"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:textAllCaps="false"/>
</RelativeLayout>
