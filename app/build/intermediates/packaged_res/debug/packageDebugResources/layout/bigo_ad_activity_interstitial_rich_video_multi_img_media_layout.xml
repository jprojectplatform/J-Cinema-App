<?xml version="1.0" encoding="utf-8"?>
<sg.bigo.ads.common.view.RoundedFrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:id="@+id/inter_media_main_container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:radius="8dp">
    <sg.bigo.ads.common.view.AdImageView
        android:id="@+id/inter_media_main_background"
        android:visibility="visible"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scaleType="centerCrop"/>
    <sg.bigo.ads.common.view.FixContentFrameLayout
        android:layout_gravity="center"
        android:id="@+id/inter_media_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:radius="8dp">
        <sg.bigo.ads.api.MediaView
            android:layout_gravity="center_horizontal"
            android:id="@+id/inter_media"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
    </sg.bigo.ads.common.view.FixContentFrameLayout>
    <Button
        android:layout_gravity="end|bottom"
        android:id="@+id/inter_btn_mute"
        android:background="@drawable/bigo_ad_ic_media_mute"
        android:visibility="invisible"
        android:layout_width="28dp"
        android:layout_height="28dp"
        android:layout_marginBottom="12dp"
        android:layout_marginEnd="12dp"/>
</sg.bigo.ads.common.view.RoundedFrameLayout>
