<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:tag="layout/fragment_mine_0"
    android:background="@color/theme_bg_color"
    android:scrollbars="none"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:orientation="vertical"
            android:background="@color/color_1b202b"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <androidx.constraintlayout.widget.ConstraintLayout
                android:tag="binding_1"
                android:paddingLeft="@dimen/dp_15"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_64"
                android:layout_marginTop="@dimen/dp_50">
                <com.mgs.carparking.widgets.CircularImageView
                    android:id="@+id/iv_header"
                    android:tag="binding_2"
                    android:layout_width="@dimen/dp_50"
                    android:layout_height="@dimen/dp_50"
                    android:src="@drawable/ic_avatar_default"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"/>
                <TextView
                    android:textSize="@dimen/sp_19"
                    android:textStyle="bold"
                    android:textColor="@color/color_text_light_5"
                    android:ellipsize="end"
                    android:id="@+id/tv_name"
                    android:tag="binding_3"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/iv_header"
                    android:layout_marginStart="@dimen/dp_15"
                    android:layout_marginEnd="@dimen/dp_20"
                    app:layout_constraintBottom_toBottomOf="@+id/iv_header"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/iv_header"
                    app:layout_constraintTop_toTopOf="@+id/iv_header"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/vipBg"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="@dimen/dp_25"
                android:layout_marginRight="15dp"
                android:layout_marginHorizontal="15dp">
                <LinearLayout
                    android:orientation="horizontal"
                    android:id="@+id/topVp"
                    android:tag="binding_4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:tag="binding_5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/dp_6"/>
                    <TextView
                        android:textSize="@dimen/sp_11"
                        android:tag="binding_6"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/dp_10"
                        android:layout_marginTop="@dimen/dp_3"
                        android:layout_marginRight="@dimen/dp_20"/>
                </LinearLayout>
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:tag="binding_7"
                    android:background="@drawable/ic_mine_bottom_vip_bg"
                    android:padding="@dimen/dp_10"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_19"
                    android:layout_alignParentBottom="true"
                    app:layout_constraintTop_toTopOf="@+id/topVp">
                    <TextView
                        android:textSize="14sp"
                        android:textStyle="bold"
                        android:textColor="@color/color_CC000000"
                        android:id="@+id/tvTopVp"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:text="@string/str_join_vp_noad"
                        android:layout_marginEnd="8dp"
                        app:layout_constraintEnd_toStartOf="@+id/llRightLayout"
                        app:layout_constraintStart_toStartOf="parent"/>
                    <TextView
                        android:textSize="12sp"
                        android:textColor="@color/color_CC000000"
                        android:id="@+id/tvBottomVp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/str_discount"
                        app:layout_constraintStart_toStartOf="@+id/tvTopVp"
                        app:layout_constraintTop_toBottomOf="@+id/tvTopVp"/>
                    <LinearLayout
                        android:orientation="vertical"
                        android:id="@+id/llRightLayout"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent">
                        <com.ruffian.library.widget.RTextView
                            android:textSize="9sp"
                            android:textColor="@color/color_main_tab_sel"
                            android:gravity="center"
                            android:paddingTop="@dimen/dp_3"
                            android:paddingBottom="@dimen/dp_3"
                            android:layout_width="82dp"
                            android:layout_height="wrap_content"
                            android:text="@string/str_limited_time"
                            android:paddingVertical="@dimen/dp_3"
                            app:background_normal="@color/color_fc511f"
                            app:corner_radius_top_left="@dimen/dp_3"
                            app:corner_radius_top_right="@dimen/dp_3"/>
                        <com.ruffian.library.widget.RTextView
                            android:textSize="14sp"
                            android:textStyle="bold"
                            android:textColor="@color/theme_bg_color"
                            android:gravity="center"
                            android:paddingTop="@dimen/dp_5"
                            android:paddingBottom="@dimen/dp_5"
                            android:layout_width="82dp"
                            android:layout_height="wrap_content"
                            android:text="@string/str_subscription"
                            android:paddingVertical="@dimen/dp_5"
                            app:background_normal="@array/array_tr_bl"
                            app:corner_radius_bottom_left="@dimen/dp_3"
                            app:corner_radius_bottom_right="@dimen/dp_3"
                            app:gradient_centerX="0.5"
                            app:gradient_orientation="LEFT_RIGHT"/>
                    </LinearLayout>
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </LinearLayout>
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="@dimen/dp_15">
            <RelativeLayout
                android:tag="binding_8"
                android:background="@drawable/bg_pressed"
                android:paddingLeft="@dimen/dp_15"
                android:paddingRight="@dimen/dp_10"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_40">
                <TextView
                    android:textSize="@dimen/sp_15"
                    android:textColor="@color/color_text_light_5"
                    android:gravity="center_vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="@string/str_share"
                    android:drawableLeft="@drawable/ic_mine_share"
                    android:drawablePadding="@dimen/dp_12"
                    android:layout_centerVertical="true"/>
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_mine_right"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"/>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/view_18"
                android:background="@drawable/bg_pressed"
                android:paddingLeft="@dimen/dp_15"
                android:paddingRight="@dimen/dp_10"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_40"
                android:layout_marginTop="@dimen/dp_10">
                <TextView
                    android:textSize="@dimen/sp_15"
                    android:textColor="@color/color_text_commen1"
                    android:gravity="center_vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="@string/str_mine_adult"
                    android:drawableLeft="@drawable/ic_mine_18"
                    android:drawablePadding="@dimen/dp_12"
                    android:layout_centerVertical="true"/>
                <Switch
                    android:id="@+id/swh_status"
                    android:layout_width="@dimen/dp_200"
                    android:layout_height="wrap_content"
                    android:textOn=""
                    android:textOff=""
                    android:thumb="@drawable/thumb_selctor"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:track="@drawable/track_selctor"/>
            </RelativeLayout>
            <RelativeLayout
                android:tag="binding_9"
                android:background="@drawable/bg_pressed"
                android:paddingLeft="@dimen/dp_15"
                android:paddingRight="@dimen/dp_10"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_40"
                android:layout_marginTop="@dimen/dp_10">
                <TextView
                    android:textSize="@dimen/sp_15"
                    android:textColor="@color/color_text_commen1"
                    android:gravity="center_vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="@string/text_mine_language_change"
                    android:drawableLeft="@drawable/ic_mine_language_change"
                    android:drawablePadding="@dimen/dp_12"
                    android:layout_centerVertical="true"/>
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_mine_right"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"/>
            </RelativeLayout>
            <RelativeLayout
                android:tag="binding_10"
                android:background="@drawable/bg_pressed"
                android:paddingLeft="@dimen/dp_15"
                android:paddingRight="@dimen/dp_10"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_40"
                android:layout_marginTop="@dimen/dp_10">
                <TextView
                    android:textSize="@dimen/sp_15"
                    android:textColor="@color/color_text_light_5"
                    android:gravity="center_vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="@string/text_mine_game"
                    android:drawableLeft="@drawable/ic_mine_mygame"
                    android:drawablePadding="@dimen/dp_12"
                    android:layout_centerVertical="true"/>
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_mine_right"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"/>
            </RelativeLayout>
            <RelativeLayout
                android:tag="binding_11"
                android:background="@drawable/bg_pressed"
                android:paddingLeft="@dimen/dp_15"
                android:paddingRight="@dimen/dp_10"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_40"
                android:layout_marginTop="@dimen/dp_10">
                <TextView
                    android:textSize="@dimen/sp_15"
                    android:textColor="@color/color_text_light_5"
                    android:gravity="center_vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="@string/text_mine_history"
                    android:drawableLeft="@drawable/ic_mine_history"
                    android:drawablePadding="@dimen/dp_12"
                    android:layout_centerVertical="true"/>
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_mine_right"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"/>
            </RelativeLayout>
            <RelativeLayout
                android:tag="binding_12"
                android:background="@drawable/bg_pressed"
                android:paddingLeft="@dimen/dp_15"
                android:paddingRight="@dimen/dp_10"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_40"
                android:layout_marginTop="@dimen/dp_10">
                <TextView
                    android:textSize="@dimen/sp_15"
                    android:textColor="@color/color_text_light_5"
                    android:gravity="center_vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="@string/text_mine_download"
                    android:drawableLeft="@drawable/ic_mine_download"
                    android:drawablePadding="@dimen/dp_12"
                    android:layout_centerVertical="true"/>
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_mine_right"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"/>
            </RelativeLayout>
            <RelativeLayout
                android:tag="binding_13"
                android:background="@drawable/bg_pressed"
                android:paddingLeft="@dimen/dp_15"
                android:paddingRight="@dimen/dp_10"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_40"
                android:layout_marginTop="@dimen/dp_10">
                <TextView
                    android:textSize="@dimen/sp_15"
                    android:textColor="@color/color_text_light_5"
                    android:gravity="center_vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="@string/text_mine_collection"
                    android:drawableLeft="@drawable/ic_mine_collection"
                    android:drawablePadding="@dimen/dp_12"
                    android:layout_centerVertical="true"/>
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_mine_right"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"/>
            </RelativeLayout>
            <RelativeLayout
                android:tag="binding_14"
                android:background="@drawable/bg_pressed"
                android:paddingLeft="@dimen/dp_15"
                android:paddingRight="@dimen/dp_10"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_40"
                android:layout_marginTop="@dimen/dp_10">
                <TextView
                    android:textSize="@dimen/sp_15"
                    android:textColor="@color/color_text_light_5"
                    android:gravity="center_vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="@string/str_order_list"
                    android:drawableLeft="@drawable/ic_mine_play"
                    android:drawablePadding="@dimen/dp_12"
                    android:layout_centerVertical="true"/>
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_mine_right"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"/>
            </RelativeLayout>
            <RelativeLayout
                android:tag="binding_15"
                android:background="@drawable/bg_pressed"
                android:paddingLeft="@dimen/dp_15"
                android:paddingRight="@dimen/dp_10"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_40"
                android:layout_marginTop="@dimen/dp_10">
                <TextView
                    android:textSize="@dimen/sp_15"
                    android:textColor="@color/color_text_light_5"
                    android:gravity="center_vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="@string/text_mine_feedback"
                    android:drawableLeft="@drawable/ic_mine_feedback"
                    android:drawablePadding="@dimen/dp_10"
                    android:layout_centerVertical="true"/>
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_mine_right"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"/>
                <View
                    android:tag="binding_16"
                    android:background="@drawable/shape_message_oval"
                    android:layout_width="@dimen/dp_8"
                    android:layout_height="@dimen/dp_8"
                    android:layout_marginRight="@dimen/dp_10"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"/>
            </RelativeLayout>
            <TextView
                android:textSize="@dimen/sp_15"
                android:textColor="@color/color_text_light_5"
                android:gravity="center_vertical"
                android:background="@drawable/bg_pressed"
                android:paddingLeft="@dimen/dp_15"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_40"
                android:layout_marginTop="@dimen/dp_15"
                android:text="@string/text_mine_privacy"
                android:drawableLeft="@drawable/ic_mine_privacy"
                android:drawablePadding="@dimen/dp_12"/>
            <RelativeLayout
                android:tag="binding_17"
                android:background="@drawable/bg_pressed"
                android:paddingLeft="@dimen/dp_15"
                android:paddingRight="@dimen/dp_10"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_40"
                android:layout_marginTop="@dimen/dp_10">
                <TextView
                    android:textSize="@dimen/sp_15"
                    android:textColor="@color/color_text_light_5"
                    android:gravity="center_vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="@string/text_mine_service"
                    android:drawableLeft="@drawable/callservice"
                    android:drawablePadding="@dimen/dp_12"
                    android:layout_centerVertical="true"/>
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_mine_right"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"/>
            </RelativeLayout>
            <RelativeLayout
                android:tag="binding_18"
                android:background="@drawable/bg_pressed"
                android:paddingLeft="@dimen/dp_15"
                android:paddingRight="@dimen/dp_10"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_40"
                android:layout_marginTop="@dimen/dp_10"
                android:layout_marginBottom="@dimen/dp_10">
                <TextView
                    android:textSize="@dimen/sp_15"
                    android:textColor="@color/color_text_light_5"
                    android:gravity="center_vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="@string/text_mine_setting"
                    android:drawableLeft="@drawable/ic_mine_setting"
                    android:drawablePadding="@dimen/dp_12"
                    android:layout_centerVertical="true"/>
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_mine_right"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"/>
            </RelativeLayout>
            <LinearLayout
                android:layout_gravity="center_horizontal"
                android:orientation="vertical"
                android:id="@+id/layout_adView"
                android:tag="binding_19"
                android:background="@color/color_text_light_5"
                android:layout_width="0dp"
                android:layout_height="0dp"/>
        </LinearLayout>
    </LinearLayout>
</ScrollView>
