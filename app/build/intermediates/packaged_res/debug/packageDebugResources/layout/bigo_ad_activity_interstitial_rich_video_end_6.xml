<?xml version="1.0" encoding="utf-8"?>
<sg.bigo.ads.api.NativeAdView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/inter_native_ad_view"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:gravity="top|center_horizontal"
        android:orientation="vertical"
        android:id="@+id/inter_media_container"
        android:clipChildren="false"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <RelativeLayout
            android:id="@+id/inter_media_ad_desc"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="20dp">
            <sg.bigo.ads.common.view.RoundedFrameLayout xmlns:app="http://schemas.android.com/apk/res-auto"
                android:layout_gravity="center"
                android:id="@+id/icon_layout"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:radius="8dp">
                <ImageView
                    android:id="@+id/inter_icon"
                    android:layout_width="32dp"
                    android:layout_height="32dp"/>
            </sg.bigo.ads.common.view.RoundedFrameLayout>
            <TextView
                android:textSize="20sp"
                android:textColor="#ffffffff"
                android:ellipsize="end"
                android:id="@+id/inter_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/bigo_ad_title_default"
                android:maxLines="1"
                android:layout_toRightOf="@+id/icon_layout"
                android:layout_centerVertical="true"
                android:fontFamily="sans-serif-medium"
                android:layout_marginStart="12dp"/>
        </RelativeLayout>
        <sg.bigo.ads.common.view.ViewFlow
            android:id="@+id/inter_media_ad_view_flow"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_marginBottom="12dp"
            android:layout_weight="1"/>
        <LinearLayout
            android:orientation="vertical"
            android:id="@+id/inter_media_ad_extra"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <sg.bigo.ads.common.view.Indicator
                android:id="@+id/vf_indicator"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
            <include
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                layout="@layout/bigo_ad_layout_interstitial_rich_video_multi_img_bottom"/>
        </LinearLayout>
    </LinearLayout>
</sg.bigo.ads.api.NativeAdView>
